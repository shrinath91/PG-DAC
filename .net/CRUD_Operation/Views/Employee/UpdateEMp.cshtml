@model Emp
<form asp-controller="Employee" asp-action="UpdateEmp" method="post">
    <div class="form-group">
        <label>Id:</label>
        <input asp-for="Id" class="form-control"></input>
    </div>
    <div class="form-group">
        <label>Name: </label>
        <input asp-for="Name" class="form-control"></input>
    </div>
    <div class="form-group">
        <label>Sal: </label>
        <input asp-for="Sal" class="form-control"></input>
    </div>
    <div class="form-group">
        <label>Address: </label>
        <input asp-for="Address" class="form-control"></input>
    </div>
    <div class="form-group">
        <lablel>Gender: </lablel>
        <select asp-for="Gender" class="form-control">
            <option value=""> Select</option>
            <option value="M">Male</option>
            <option value="F">Female</option>
        </select>
    </div>
    <div class="form-group">
        <input type="submit" value="Update" ></input>
    </div>
</form>